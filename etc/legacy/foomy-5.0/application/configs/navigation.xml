<?xml version="1.0" encoding="UTF-8"?>
<config>
  <nav>
    <blog>
      <label>IMHO</label>
      <uri>/blog/</uri>
      <module>blog</module>
      <controller>index</controller>
      <id>blog</id>
      <pages>
        <new>
          <label>Neuer Artikel</label>
          <uri>/blog/article/new/</uri>
          <module>blog</module>
          <controller>article</controller>
          <action>new</action>
          <id>article</id>
        </new>
      </pages>
    </blog>
    <bookmarks>
      <label>Bookmarks</label>
      <uri>/bookmarks/</uri>
      <module>bookmarks</module>
      <controller>index</controller>
      <id>bookmark</id>
    </bookmarks>
    <admin>
      <label>Admin</label>
      <uri>/admin/quotes/</uri>
      <module>admin</module>
      <controller>quotes</controller>
      <id>admin</id>
      <pages>
        <quotes>
          <label>Zitate</label>
          <uri>/admin/quotes/</uri>
          <module>admin</module>
          <controller>quotes</controller>
          <id>quotes</id>
        </quotes>
        <users>
          <label>Benutzer</label>
          <uri>/admin/users/</uri>
          <module>admin</module>
          <controller>users</controller>
          <id>users</id>
        </users>
        <roles>
          <label>Gruppen</label>
          <uri>/admin/roles/</uri>
          <module>admin</module>
          <controller>roles</controller>
          <id>roles</id>
        </roles>
      </pages>
    </admin>
    <bastel>
      <label>Bastelstube</label>
      <uri>/baste/</uri>
      <controller>bastel</controller>
      <id>bastel</id>
    </bastel>
  </nav>
</config>
